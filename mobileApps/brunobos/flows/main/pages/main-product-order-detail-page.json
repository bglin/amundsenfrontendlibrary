{
  "pageModelVersion": "19.1.3",
  "title": "ProductOrder Detail",
  "description": "Displays record for ProductOrder",
  "variables": {
    "productOrder": {
      "type": "page:getProductOrderResponse"
    },
    "productOrderId": {
      "type": "string",
      "required": true,
      "input": "fromUrl"
    },
    "productOrderLineListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_ProductOrderLine",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallProductOrderLineResponse",
        "filterCriterion": {
          "criteria": [
            {
              "attribute": "productOrder",
              "op": "$eq",
              "value": "{{ $variables.productOrderId }}"
            }
          ]
        }
      }
    }
  },
  "types": {
    "getProductOrderResponse": {
      "id": "number",
      "associate": "number",
      "orderDate": "string",
      "orderStatus": "string"
    },
    "@decorators.getProductOrderResponse": {
      "endpoint": "get_ProductOrder",
      "service": "businessObjects",
      "path": "response"
    },
    "getallProductOrderLineResponse": {
      "items": [
        {
          "id": "number",
          "unitPrice": "number",
          "quantity": "number",
          "productObject": {
            "items": [
              {
                "productName": "string"
              }
            ]
          }
        }
      ]
    },
    "@decorators.getallProductOrderLineResponse": {
      "endpoint": "getall_ProductOrderLine",
      "service": "businessObjects",
      "path": "response"
    }
  },
  "chains": {
    "goBackChain": {
      "root": "navigateBack",
      "actions": {
        "navigateBack": {
          "module": "vb/action/builtin/navigateBackAction",
          "outcomes": {
            "success": ""
          }
        }
      }
    },
    "loadProductOrderChain": {
      "root": "loadProductOrder",
      "actions": {
        "loadProductOrder": {
          "module": "vb/action/builtin/restAction",
          "parameters": {
            "endpoint": "businessObjects/get_ProductOrder",
            "uriParams": {
              "ProductOrder_Id": "{{ $page.variables.productOrderId }}"
            },
            "responseType": "page:getProductOrderResponse"
          },
          "outcomes": {
            "success": "assignPayload",
            "failure": "fireErrorNotification"
          }
        },
        "assignPayload": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.productOrder": {
              "source": "{{ $chain.results.loadProductOrder.body || $chain.results.loadProductOrder.content }}"
            }
          },
          "outcomes": {}
        },
        "fireErrorNotification": {
          "module": "vb/action/builtin/fireNotificationEventAction",
          "parameters": {
            "summary": "Could not load data",
            "message": "{{ 'Could not load data: status ' + $chain.results.loadProductOrder.payload.status }}",
            "type": "error",
            "displayMode": "persist"
          },
          "outcomes": {}
        }
      }
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chainId": "goBackChain"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chainId": "loadProductOrderChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-ext-mobile-page-template": {
        "path": "vb/components/mobile-page-template/loader"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-input-date-time": {
        "path": "ojs/ojdatetimepicker"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      }
    }
  }
}