{
  "pageModelVersion": "19.1.3",
  "title": "Store Detail",
  "description": "Displays record for Store",
  "variables": {
    "store": {
      "type": "page:getStoreResponse"
    },
    "storeId": {
      "type": "string",
      "required": true,
      "input": "fromUrl"
    },
    "associateListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "mmassociates/getall_Associate",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallAssociateResponse",
        "filterCriterion": {
          "criteria": [
            {
              "attribute": "store",
              "op": "$eq",
              "value": "{{ $variables.store.id }}"
            }
          ]
        }
      }
    }
  },
  "types": {
    "getStoreResponse": {
      "id": "number",
      "name": "string",
      "address": "string",
      "city": "string",
      "state": "string"
    },
    "@decorators.getStoreResponse": {
      "endpoint": "get_Store",
      "service": "mmastores",
      "path": "response"
    },
    "getallAssociateResponse": {
      "items": [
        {
          "id": "number",
          "name": "string",
          "hireDate": "string",
          "email": "string"
        }
      ]
    },
    "@decorators.getallAssociateResponse": {
      "endpoint": "getall_Associate",
      "service": "mmassociates",
      "path": "response"
    }
  },
  "chains": {
    "goBackChain": {
      "root": "navigateBack",
      "actions": {
        "navigateBack": {
          "module": "vb/action/builtin/navigateBackAction",
          "outcomes": {
            "success": ""
          }
        }
      }
    },
    "loadStoreChain": {
      "root": "loadStore",
      "actions": {
        "loadStore": {
          "module": "vb/action/builtin/restAction",
          "parameters": {
            "endpoint": "mmastores/get_Store",
            "uriParams": {
              "Store_Id": "{{ $page.variables.storeId }}"
            },
            "responseType": "page:getStoreResponse"
          },
          "outcomes": {
            "success": "assignPayload",
            "failure": "fireErrorNotification"
          }
        },
        "assignPayload": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.store": {
              "source": "{{ $chain.results.loadStore.body || $chain.results.loadStore.content }}"
            }
          },
          "outcomes": {}
        },
        "fireErrorNotification": {
          "module": "vb/action/builtin/fireNotificationEventAction",
          "parameters": {
            "summary": "Could not load data",
            "message": "{{ 'Could not load data: status ' + $chain.results.loadStore.payload.status }}",
            "type": "error",
            "displayMode": "persist"
          },
          "outcomes": {}
        }
      }
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chainId": "goBackChain"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chainId": "loadStoreChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      }
    }
  }
}