{
  "pageModelVersion": "19.1.3",
  "title": "",
  "description": "",
  "variables": {
    "productListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Product",
        "idAttribute": "id",
        "itemsPath": "items",
        "responseType": "page:getallProductResponse"
      }
    },
    "oj_table_448516599_1SelectedId": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "types": {
    "getallProductResponse": {
      "items": [
        {
          "id": "number",
          "productName": "string",
          "productDescription": "string",
          "unitPrice": "number"
        }
      ]
    },
    "@decorators.getallProductResponse": {
      "endpoint": "getall_Product",
      "service": "businessObjects",
      "path": "response"
    }
  },
  "chains": {
    "navigateToProductDetailChain": {
      "root": "navigateToDetail",
      "variables": {
        "productId": {
          "type": "string",
          "input": "fromCaller",
          "required": true
        }
      },
      "actions": {
        "navigateToDetail": {
          "module": "vb/action/builtin/navigateToPageAction",
          "parameters": {
            "history": "push",
            "page": "main-product-detail",
            "params": {
              "productId": "[[ $chain.variables.productId ]]"
            }
          },
          "outcomes": {
            "success": ""
          }
        }
      }
    },
    "oj_table_448516599_1ChangeSelectionChain": {
      "variables": {
        "productId": {
          "type": "string",
          "input": "fromCaller",
          "defaultValue": ""
        }
      },
      "root": "assignVariable",
      "actions": {
        "assignVariable": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.oj_table_448516599_1SelectedId": {
              "source": "[[ $chain.variables.productId ]]"
            }
          },
          "outcomes": {
            "success": ""
          }
        }
      }
    }
  },
  "eventListeners": {
    "oj_table_448516599_1SelectionChanged": {
      "chains": [
        {
          "chainId": "oj_table_448516599_1ChangeSelectionChain",
          "parameters": {
            "productId": "[[ ($event.detail.value && $event.detail.value[0] && 'row' in $event.detail.value[0].startKey) ? $event.detail.value[0].startKey.row : '' ]]"
          }
        }
      ]
    },
    "productDetailButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToProductDetailChain",
          "parameters": {
            "productId": "[[ $page.variables.oj_table_448516599_1SelectedId ]]"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      }
    }
  }
}