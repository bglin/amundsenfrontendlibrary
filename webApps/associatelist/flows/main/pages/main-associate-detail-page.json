{
  "pageModelVersion": "19.1.3",
  "title": "Associate Detail",
  "description": "Displays record for Associate",
  "variables": {
    "associate": {
      "type": "page:getAssociateResponse"
    },
    "associateId": {
      "type": "string",
      "required": true,
      "input": "fromUrl"
    }
  },
  "types": {
    "getAssociateResponse": {
      "id": "number",
      "name": "string",
      "store": "number",
      "email": "string"
    },
    "@decorators.getAssociateResponse": {
      "endpoint": "get_Associate",
      "service": "mmassociates",
      "path": "response"
    }
  },
  "chains": {
    "goBackChain": {
      "root": "navigateBack",
      "actions": {
        "navigateBack": {
          "module": "vb/action/builtin/navigateBackAction",
          "outcomes": {
            "success": ""
          }
        }
      }
    },
    "loadAssociateChain": {
      "root": "loadAssociate",
      "actions": {
        "loadAssociate": {
          "module": "vb/action/builtin/restAction",
          "parameters": {
            "endpoint": "mmassociates/get_Associate",
            "uriParams": {
              "Associate_Id": "{{ $page.variables.associateId }}"
            },
            "responseType": "page:getAssociateResponse"
          },
          "outcomes": {
            "success": "assignPayload",
            "failure": "fireErrorNotification"
          }
        },
        "assignPayload": {
          "module": "vb/action/builtin/assignVariablesAction",
          "parameters": {
            "$page.variables.associate": {
              "source": "{{ $chain.results.loadAssociate.body || $chain.results.loadAssociate.content }}"
            }
          },
          "outcomes": {}
        },
        "fireErrorNotification": {
          "module": "vb/action/builtin/fireNotificationEventAction",
          "parameters": {
            "summary": "Could not load data",
            "message": "{{ 'Could not load data: status ' + $chain.results.loadAssociate.payload.status }}",
            "type": "error",
            "displayMode": "persist"
          },
          "outcomes": {}
        }
      }
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chainId": "goBackChain"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chainId": "loadAssociateChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      }
    }
  }
}